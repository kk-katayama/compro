#include <iostream>
#include <algorithm>
#include <vector>
#define rep(i,n) for(int i=0;i<n;++i)
#define rep1(i,n) for(int i=1;i<=n;++i)
using namespace std;
typedef long long ll;
int n;
ll a[2100];
ll rec(int i,int j,int c){
  if(i==j) return
}
int main()
{
  cin >> n;
  rep(i,n) cin >> a[i];
  return 0;
}



